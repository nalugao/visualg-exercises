algoritmo "conversorMoedas"
var
  R, D: REAL
  contador, N: INTEIRO
inicio
contador <- 1
Escreva ("Quantas conversões você quer fazer? ")
Leia (N)
Enquanto (contador <= N) faca
   Escreva ("Qual o valor em R$? ")
   Leia (R)
   D <- R/5.75
   Escreval ("O valor convertido é US$ ", D:5:2)
   contador <- contador +1
FimEnquanto
fimalgoritmo